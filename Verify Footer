WebUI.executeJavaScript('document.body.style.zoom='67%'', null)

'Footer'
TestObject footer = new TestObject()
footer.addProperty("xpath", ConditionType.EQUALS, "/html/body/footer")
WebUI.scrollToElement(footer, 0)
WebUI.verifyElementPresent(footer, 30)
WebUI.delay(5)
screenshotImgXyz()

WebUI.closeBrowser()
